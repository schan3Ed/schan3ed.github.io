    "exif-orientation": "^1.0.0",
    "exif-orientation-image": "^1.0.1",
    "firebase": "^4.4.0",
    "image-compressor.js": "^1.1.4",
    "joi-browser": "^13.4.0",
    "material-ui": "^0.20.2",
    "material-ui-pagination": "^1.1.7",
    "material-ui-search-bar": "^0.4.2",
    "material-ui-search-bar-enhanced": "^0.5.1",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
